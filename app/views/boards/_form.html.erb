<%= form_for @board do |f| %>
    <div class="row">
      <% if @board.errors.any? %>
          <div id="error_explanation">
            <h2>
              <%= pluralize(@board.errors.count, "error") %> prohibited
              this project from being saved:
            </h2>
            <ul class="<%= flash_class(:warning) %>">
              <% @board.errors.full_messages.each do |msg| %>
                  <li><%= msg %></li>
              <% end %>
            </ul>
          </div>
      <% end %>
      <% flash.each do |key, value| %>
          <div class="text-center <%= flash_class(key) %>">
            <%= value %>
          </div>
      <% end %>
      <div class="col-lg-12 form_container">
        <!-- col-lg-12 start here -->
        <div class="panel panel-default toggle panelMove panelClose panelRefresh" id="dyn_0">
          <!-- Start .panel -->
          <div class="panel-heading">

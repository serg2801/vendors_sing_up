<div class="row">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

<div class="col-lg-12 form_container">
    <!-- col-lg-12 start here -->
    <div class="panel panel-default toggle panelMove panelClose panelRefresh log_in_panel" id="dyn_0">
      <!-- Start .panel -->
      <div class="panel-heading">
        <h4 class="panel-title">Log in</h4>
      </div>

      <div class="form-group log_in_form">
        <%= f.label :email, "Email:", :class => "col-lg-2 col-md-3 control-label" %>
        <div class="col-lg-10 col-md-9">
          <%= f.text_field :email, autofocus: true, :class => "form-control" %>
        </div>
      </div>

      <div class="form-group log_in_form">
        <%= f.label :password, "Password:", :class => "col-lg-2 col-md-3 control-label" %>
        <div class="col-lg-10 col-md-9">
          <%= f.password_field :password, autocomplete: "off", :class => "form-control" %>
        </div>
        <div class="form-group submit_log_in">
          <% if devise_mapping.rememberable? -%>
              <div class="field">
                <%= f.check_box :remember_me %>
                <%= f.label :remember_me %>
              </div>
          <% end %>
        </div>
        </div>

      <div class="actions form-group submit_log_in">
        <%= f.submit "Log in", class: 'btn' %>
      </div>

    </div>
  </div>
  <% end %>
</div>

<%#= render "devise/shared/links" %>
